var array = [4,5,6,7,8];
var singleVal = 0;

// Only change code below this line.

firstVal = array[0];
secondVal = array[1];
thirdVal = array[2];
fourthVal = array[3];
fifthVal = array[4];

var singleVal = array.reduce(function(val) {
  return firstVal + secondVal + thirdVal + fourthVal + fifthVal;
  });